{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es6",
    "allowSyntheticDefaultImports": true,
    "module": "commonjs",
    "sourceMap": true,
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "experimentalDecorators": true,
    "noLib": false,
    "jsx": "react",
    "strictNullChecks": true
  },
  "exclude": [
    "node_modules",
    "test",
    "static"
  ]
}